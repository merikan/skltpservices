<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<mule xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:jdbc="http://www.mulesoft.org/schema/mule/jdbc"
  xmlns:spring="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd   http://www.mulesoft.org/schema/mule/core    http://www.mulesoft.org/schema/mule/core/3.2/mule.xsd   http://www.mulesoft.org/schema/mule/jdbc    http://www.mulesoft.org/schema/mule/jdbc/3.2/mule-jdbc.xsd  ">

  <jdbc:connector dataSource-ref="soitoolkit-jdbc-datasource" name="jdbcConnector" pollingFrequency="${SOITOOLKIT_JDBC_CON_POLLING_MS}">
    <jdbc:query key="activelogtrigger-export-query" value="SELECT id, serviceUrl FROM ServiceProducer" />
  </jdbc:connector>

</mule>
