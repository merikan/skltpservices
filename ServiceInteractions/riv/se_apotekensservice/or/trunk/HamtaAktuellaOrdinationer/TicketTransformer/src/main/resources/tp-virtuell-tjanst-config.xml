<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:spring="http://www.springframework.org/schema/beans"
	xmlns:cxf="http://www.mulesource.org/schema/mule/cxf/2.2" xmlns:https="http://www.mulesource.org/schema/mule/https/2.2"
	xsi:schemaLocation="
               http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
               http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
               http://www.mulesource.org/schema/mule/https/2.2 http://www.mulesource.org/schema/mule/https/2.2/mule-https.xsd
               http://www.mulesource.org/schema/mule/cxf/2.2  http://www.mulesource.org/schema/mule/cxf/2.2/mule-cxf.xsd">

	<!-- Name of this file is mandatory and may not be changed. -->

	<model name="HamtaAktuellaOrdinationer-TicketTransformerService-model">

		<service
			name="HamtaAktuellaOrdinationer-TicketTransformerService-service">
			<inbound>
				<cxf:inbound-endpoint
					address="http://localhost:10000/tickettransformer/HamtaAktuellaOrdinationerResponder/V1"
					proxy="true" payload="envelope" synchronous="true"
					applyTransformersToProtocol="false"
					transformer-refs="ArgosHeaderToSamlTicket" />
			</inbound>
			<outbound>
				<pass-through-router>
					<cxf:outbound-endpoint
						address="${endpoint.apotekensservice.or.hamtaaktuellaordinationer}"
						synchronous="true" proxy="true" payload="envelope"
						protocolConnector="VPConsumerConnector" transformer-refs="addContentTypeXmlUtf8"/>
				</pass-through-router>
			</outbound>
		</service>
				
		<service
			name="HamtaAktuellaOrdinationer-TicketTransformerService-ext0">
			<inbound>
				<cxf:inbound-endpoint
					address="http://localhost:10000/testenvironment/ext0/tickettransformer/HamtaAktuellaOrdinationerResponder/V1"
					proxy="true" payload="envelope" synchronous="true"
					applyTransformersToProtocol="false"
					transformer-refs="ArgosHeaderToSamlTicket" />
			</inbound>
			<outbound>
				<pass-through-router>
					<cxf:outbound-endpoint
						address="https://stpwsp-exttest0.receptpartner.sjunet.org:42901/serviceProvider/HamtaAktuellaOrdinationerResponder/V1"
						synchronous="true" proxy="true" payload="envelope"
						protocolConnector="VPConsumerConnector" transformer-refs="addContentTypeXmlUtf8"/>
				</pass-through-router>
			</outbound>
		</service>
		
		<service
			name="HamtaAktuellaOrdinationer-TicketTransformerService-ext8">
			<inbound>
				<cxf:inbound-endpoint
					address="http://localhost:10000/testenvironment/ext8/tickettransformer/HamtaAktuellaOrdinationerResponder/V1"
					proxy="true" payload="envelope" synchronous="true"
					applyTransformersToProtocol="false"
					transformer-refs="ArgosHeaderToSamlTicket" />
			</inbound>
			<outbound>
				<pass-through-router>
					<cxf:outbound-endpoint
						address="https://ext08-02.receptpartner.sjunet.org:42901/serviceProvider/HamtaAktuellaOrdinationerResponder/V1"
						synchronous="true" proxy="true" payload="envelope"
						protocolConnector="VPConsumerConnector" transformer-refs="addContentTypeXmlUtf8"/>
				</pass-through-router>
			</outbound>
		</service>
		
		<service
			name="HamtaAktuellaOrdinationer-TicketTransformerService-ext11">
			<inbound>
				<cxf:inbound-endpoint
					address="http://localhost:10000/testenvironment/ext11/tickettransformer/HamtaAktuellaOrdinationerResponder/V1"
					proxy="true" payload="envelope" synchronous="true"
					applyTransformersToProtocol="false"
					transformer-refs="ArgosHeaderToSamlTicket" />
			</inbound>
			<outbound>
				<pass-through-router>
					<cxf:outbound-endpoint
						address="https://ext11-02.receptpartner.sjunet.org:42901/serviceProvider/HamtaAktuellaOrdinationerResponder/V1"
						synchronous="true" proxy="true" payload="envelope"
						protocolConnector="VPConsumerConnector" transformer-refs="addContentTypeXmlUtf8"/>
				</pass-through-router>
			</outbound>
		</service>

		<service name="HamtaAktuellaOrdinationer-TicketTransformerService-service-local">
			<inbound>
				<cxf:inbound-endpoint
					address="http://localhost:10000/transformer/apotekensservice/or/SamlTicketTransformer/local"
					proxy="true" payload="envelope" synchronous="true"
					applyTransformersToProtocol="false" transformer-refs="ArgosHeaderToSamlTicket" />
			</inbound>

			<outbound>
				<pass-through-router>
					<cxf:outbound-endpoint
						address="${endpoint.apotekensservice.or.hamtaaktuellaordinationer}"
						synchronous="true" proxy="true" payload="envelope" transformer-refs="addContentTypeXmlUtf8"/>
				</pass-through-router>
			</outbound>
		</service>
	</model>

</mule>
