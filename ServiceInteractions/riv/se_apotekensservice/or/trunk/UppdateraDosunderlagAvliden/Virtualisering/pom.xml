<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>se.skl.skltpservices.se_apotekensservice.or</groupId>
	<artifactId>se_apotekensservice-or-UppdateraDosunderlagAvliden-virtualisering</artifactId>
	<version>1.0-SNAPSHOT</version>
	<name>UppdateraDosunderlagAvliden service virtualisering</name>
	<url>http://maven.apache.org</url>

	<parent>
		<groupId>se.skl.skltpservices.se_apotekensservice.or</groupId>
		<artifactId>UppdateraDosunderlagAvliden</artifactId>
		<version>1.0-SNAPSHOT</version>
	</parent>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.5</version>
				<executions>
					<execution>
						<id>copy-service-schema-files</id>
						<goals>
							<goal>copy-resources</goal>
						</goals>
						<phase>generate-sources</phase>
						<configuration>
							<outputDirectory>src/main/resources/schemas</outputDirectory>
							<resources>
								<resource>
									<directory>${project.build.directory}/schemas</directory>
									<filtering>false</filtering>
								</resource>
							</resources>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

</project>
