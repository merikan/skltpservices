<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>se.skl.skltpservices.itintegration.engagementindex</groupId>
	<artifactId>engagementindex-schemas</artifactId>
	<name>engagementindex schemas</name>
	<url>http://maven.apache.org</url>

	<parent>
		<artifactId>itintegration-engagementindex</artifactId>
		<groupId>se.skl.skltpservices</groupId>
		<version>1.1-SNAPSHOT</version>
	</parent>

	<properties>
		<schema.path>schemas</schema.path>
	</properties>

	<dependencies>
		<!-- cxf dependencies -->
		<dependency>
			<groupId>org.apache.cxf</groupId>
			<artifactId>cxf-rt-frontend-jaxws</artifactId>
			<version>${cxf.version}</version>
		</dependency>
		<dependency>
			<groupId>org.apache.cxf</groupId>
			<artifactId>cxf-rt-transports-http</artifactId>
			<version>${cxf.version}</version>
		</dependency>
		<dependency>
			<groupId>org.apache.cxf</groupId>
			<artifactId>cxf-rt-transports-http-jetty</artifactId>
			<version>${cxf.version}</version>
		</dependency>

		<!-- fï¿½r att undvika att 2.0.8 laddas in -->
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-context</artifactId>
			<version>2.5.6</version>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-web</artifactId>
			<version>2.5.6</version>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.cxf</groupId>
				<artifactId>cxf-codegen-plugin</artifactId>
				<version>${cxf.version}</version>
				<executions>
					<execution>
						<id>generate-sources</id>
						<phase>generate-sources</phase>
						<configuration>
							<sourceRoot>${basedir}/target/generated/src/main/java</sourceRoot>
							<wsdlOptions>
								<wsdloption>
									<wsdl>${schema.path}/./interactions/FindContentInteraction/FindContentInteraction_1.0_RIVTABP21.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:FindContentResponder:1=se.riv.itintegration.engagementindex.findcontentresponder.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:FindContentInitiator:1=se.riv.itintegration.engagementindex.findcontentinitiator.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:FindContent:1:rivtabp21=se.riv.itintegration.engagementindex.findcontent.v1.rivtabp21</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
									</extraargs>
								</wsdloption>
								<wsdloption>
									<wsdl>${schema.path}/./interactions/ProcessNotificationInteraction/ProcessNotificationInteraction_1.0_RIVTABP21.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:ProcessNotificationResponder:1=se.riv.itintegration.engagementindex.processnotificationresponder.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:ProcessNotificationInitiator:1=se.riv.itintegration.engagementindex.processnotificationinitiator.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:ProcessNotification:1:rivtabp21=se.riv.itintegration.engagementindex.processnotification.v1.rivtabp21</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
									</extraargs>
								</wsdloption>
								<wsdloption>
									<wsdl>${schema.path}/./interactions/UpdateInteraction/UpdateInteraction_1.0_RIVTABP21.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:UpdateResponder:1=se.riv.itintegration.engagementindex.updateresponder.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:UpdateInitiator:1=se.riv.itintegration.engagementindex.updateinitiator.v1</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:engagementindex:Update:1:rivtabp21=se.riv.itintegration.engagementindex.update.v1.rivtabp21</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
									</extraargs>
								</wsdloption>
							</wsdlOptions>
						</configuration>
						<goals>
							<goal>wsdl2java</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

</project>
