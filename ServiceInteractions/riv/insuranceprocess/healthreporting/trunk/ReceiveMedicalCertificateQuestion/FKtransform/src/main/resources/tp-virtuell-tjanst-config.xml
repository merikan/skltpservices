<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:spring="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:cxf="http://www.mulesource.org/schema/mule/cxf/2.2" 
	xmlns:http="http://www.mulesource.org/schema/mule/http/2.2"
	xmlns:https="http://www.mulesource.org/schema/mule/https/2.2"
	xsi:schemaLocation="
               http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
               http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
               http://www.springframework.org/schema/context   http://www.springframework.org/schema/context/spring-context-2.5.xsd
               http://www.mulesource.org/schema/mule/http/2.2 http://www.mulesource.org/schema/mule/http/2.2/mule-http.xsd
               http://www.mulesource.org/schema/mule/https/2.2 http://www.mulesource.org/schema/mule/https/2.2/mule-https.xsd
               http://www.mulesource.org/schema/mule/cxf/2.2  http://www.mulesource.org/schema/mule/cxf/2.2/mule-cxf.xsd">
	<!--
	Name of this file is mandatory and may not be changed. 
	 -->
	
	<custom-transformer name="vard2fk" class="se.skl.tp.recmedcertquestion.transformers.Vard2FkTransformer"/>
	<custom-transformer name="fk2vard" class="se.skl.tp.recmedcertquestion.transformers.Fk2VardTransformer"/>
	
	<model name="fk-varden-model">
		<service name="fk-varden-service">
			<inbound>
				<cxf:inbound-endpoint 
					address="http://localhost:10000/transformVardFk"
					wsdlLocation="classpath:/schemas/varden/ProcessRequestInteraction_0.8_ifv_rivtabp20.wsdl"
					serviceName="ProcessRequestResponderService"
					namespace="urn:riv:fk:sicknessreport:ProcessRequest:1:rivtabp20"
					proxy="true" 
					synchronous="true"
					applyTransformersToProtocol="false"
					payload="envelope"
					transformer-refs="vard2fk"
					responseTransformer-refs="fk2vard"
				/>							
			</inbound>

			<outbound>
                <pass-through-router>
                        <cxf:outbound-endpoint 
                        	address="https://rivsat.forsakringskassan.sjunet.org/ws" 
                        	wsdlLocation="classpath:/schemas/fk/SkickaLakarintygInteraction_0.9_fk_rivtabp20.wsdl"
                        	wsdlPort="SkickaLakarintygResponderPort"
                        	operation="SkickaLakarintyg" 
                        	clientClass="se.skl.riv.fk.varden.v1.rivtabp20.skickalakarintyg.SkickaLakarintygResponderService"
            	        	synchronous="true"
                        /> 
                </pass-through-router>
            </outbound>
		</service>
		
	</model>
</mule>
