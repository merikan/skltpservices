<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:spring="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:cxf="http://www.mulesource.org/schema/mule/cxf/2.2" 
	xmlns:http="http://www.mulesource.org/schema/mule/http/2.2"
	xmlns:https="http://www.mulesource.org/schema/mule/https/2.2"
	xsi:schemaLocation="
               http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
               http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
               http://www.springframework.org/schema/context   http://www.springframework.org/schema/context/spring-context-2.5.xsd
               http://www.mulesource.org/schema/mule/http/2.2 http://www.mulesource.org/schema/mule/http/2.2/mule-http.xsd
               http://www.mulesource.org/schema/mule/https/2.2 http://www.mulesource.org/schema/mule/https/2.2/mule-https.xsd
               http://www.mulesource.org/schema/mule/cxf/2.2  http://www.mulesource.org/schema/mule/cxf/2.2/mule-cxf.xsd">
	<!--
	Name of this file is mandatory and may not be changed. 
	 -->
	<custom-transformer name="FkReq2VardAnswer" class="se.skl.tp.taemotsvar.transformers.FkRequest2VardTransformer"/>
	<custom-transformer name="VardResp2FkAnswer" class="se.skl.tp.taemotsvar.transformers.VardResponse2FkTransformer"/>
	
	<model name="fk2varden-answer-model">
		<service name="fk2varden-answer-service">
			<inbound>
				<cxf:inbound-endpoint 
					address="http://localhost:10000/transformFkVardAnswer"
					wsdlLocation="classpath:/schemas/fk/TaEmotSvarInteraction_0.9_rivtabp20.wsdl"
					serviceName="TaEmotSvarResponderService"
					namespace="urn:riv:fk:vardgivare:sjukvard:TaEmotSvar:1:rivtabp20"
					proxy="true" 
					synchronous="true"
					applyTransformersToProtocol="false"
					transformer-refs="FkReq2VardAnswer"
					responseTransformer-refs="VardResp2FkAnswer"
				/>							
			</inbound>

			<outbound>
                <pass-through-router>
                        <cxf:outbound-endpoint 
                        	address="http://192.168.16.211:8080/casebox/ws/RecMedCertAnswer/1/rivtabp20" 
							wsdlLocation="classpath:/schemas/vard/ReceiveMedicalCertificateAnswerInteraction_0.9_rivtabp20.wsdl"
                        	wsdlPort="ReceiveMedicalCertificateAnswerResponderPort"
                        	operation="ReceiveMedicalCertificateAnswer" 
                        	clientClass="se.skl.riv.insuranceprocess.healthreporting.receivemedicalcertificateanswer.v1.rivtabp20.ReceiveMedicalCertificateAnswerResponderService"
            	        	synchronous="true"
                        /> 
                </pass-through-router>
            </outbound>
		</service>
		
	</model>
</mule>
